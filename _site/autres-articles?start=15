	<div class="page-header">
					<h2 itemprop="name">
									<a href="/autres-articles/211-talkmyphone-envoi-sms-pc" itemprop="url">
						Talkmyphone pour Android					</a>
							</h2>
				
		
				
			</div>

	
<div class="icons">
	
					<div class="btn-group pull-right">
