	<div class="page-header">
					<h2 itemprop="name">
									<a href="/autres-articles/61-insert-sql-automatiques" itemprop="url">
						Insertions mySQL faciles					</a>
							</h2>
				
		
				
			</div>

	
<div class="icons">
	
					<div class="btn-group pull-right">
				<a class="btn dropdown-toggle" data-toggle="dropdown" href="#"> <span class="icon-cog"></span><span class="caret"></span> </a>
								<ul class="dropdown-menu">
																<li class="email-icon"> <a href="/component/mailto/?tmpl=component&amp;template=protostar&amp;link=fcd4a11b4297cc35126c24463e3ff7966dfbb059" title="Email this link to a friend" onclick="window.open(this.href,'win2','width=400,height=350,menubar=yes,resizable=yes'); return false;" rel="nofollow"><span class="icon-envelope"></span>Email</a> </li>
